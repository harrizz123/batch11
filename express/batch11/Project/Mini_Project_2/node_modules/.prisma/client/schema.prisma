datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Products {
  product_id        String @unique @default(uuid())
  product_name      String
  product_rating    String
  product_pricing   Int
  product_location  String
  product_image_url String
}
